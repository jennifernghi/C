#include <stdio.h>
#include <stdlib.h>
int cubeByValue(int num);
int cubeByReference(int *npt);
int main(void) {
	int a =7;
	int *apt = &a;

	printf("address of a %p\nvalue of apt %p", &a, apt);
	printf("\nvalue of a %d\nvalue of *apt %d", a, *apt);
	printf("\nshowing that & and * are complement of the other\n"
			"&*apt = %p\n"
			"*&apt = %p\n", &*apt, *&apt);
}

int cubeByValue(int num){
	return num*num*num;
}
